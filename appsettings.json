{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "IdentityDataContextConnection": "Server=(localdb)\\mssqllocaldb;Database=style-catalog;Trusted_Connection=True;MultipleActiveResultSets=true",
    "AccountContextConnection": "Server=(localdb)\\mssqllocaldb;Database=style-catalog;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "SassCompiler": {
    "SourceFolder": "Styles",
    "TargetFolder": "wwwroot/css",
    "Arguments": "--style=compressed",
    "GenerateScopedCss": true,
    "ScopedCssFolders": [
      "Views",
      "Pages",
      "Shared",
      "Components"
    ]
  }
}